                            //add columns
                            for (int index = 0; index < firstArrayCountx; index++)
                            {
                                dtx.Columns.Add(new DataColumn());
                            }
                            do
                            {
                            linefinal = FileDelimiterx + lvClientCode + FileDelimiterx + lvintFileInfoId + FileDelimiterx + lvintFileTrackId + FileDelimiterx + lvFileName + FileDelimiterx;
                            for (int index = 0; index < firstArrayCountx; index++)
                                {
                                string xx = strArrayx[index].ToString();
                                string[] stringSeparatorsxy = new string[] { ";" };
                                string[] strArrayxy = xx.Split(stringSeparatorsxy, StringSplitOptions.None);
                                linefinal = linefinal + linex.Substring((Convert.ToInt32(strArrayxy[0].ToString()) - 1), ((Convert.ToInt32(strArrayxy[1].ToString()) + 1) - Convert.ToInt32(strArrayxy[0].ToString()))) + FileDelimiterx;
                                }
                            DataRow rowx = dtx.NewRow();
                            string[] itemArrayABC = linefinal.Split(stringSeparatorsABC, StringSplitOptions.None);
                            rowx.ItemArray = itemArrayABC;
                            dtx.Rows.Add(rowx);
                            linex = srx.ReadLine();
                            } while (!string.IsNullOrEmpty(linex));
                        srx.Close();

                        var bcx = new SqlBulkCopy(dbConn, SqlBulkCopyOptions.TableLock, null)
                        {
                            DestinationTableName = TableName,
