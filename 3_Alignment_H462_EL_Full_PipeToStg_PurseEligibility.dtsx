                        }
                        string[] stringSeparatorsABC = new string[] { "|" };
                            //for column loop
                            string[] stringSeparatorsx = new string[] { "|" };
                            string[] strArrayx = strFixedLengthPos.Split(stringSeparatorsx, StringSplitOptions.None);
