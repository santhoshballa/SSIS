        public void Main()
        {
            // C# Script for Script Task in SSIS
            String SendMailFrom = Dts.Variables["User::strFromEmailAddress"].Value.ToString();
            String SendMailTo = Dts.Variables["User::strToEmailAddress"].Value.ToString();
            String SendMailSubject = Dts.Variables["User::strEmailSubject"].Value.ToString();
            String SendMailBody = Dts.Variables["User::strEmailBody"].Value.ToString();
            String SendMailPswd = Dts.Variables["User::strEmailPassword"].Value.ToString();
            String SmtpServerName = Dts.Variables["User::strSmtpServerName"].Value.ToString();
            String SourceFolderPath = Dts.Variables["User::strWinSCPDestinationPath"].Value.ToString();
            String IsSecureEmail = Dts.Variables["User::strIsSecureEmail"].Value.ToString();
            String SourceFileName = Dts.Variables["User::strWinSCPSourceFileName"].Value.ToString();
            String IsAttachFile = Dts.Variables["User::strIsAttachFile"].Value.ToString();
            

            //User::strEmailBody,User::strEmailPassword,User::strEmailSubject,User::strFromEmailAddress,User::strFullFileNames,User::strIsAttachFile,User::strIsEmailNofification,User::strIsSecureEmail,User::strSmtpServerName,User::strToEmailAddress,User::strWinSCPDestinationPath,User::strWinSCPSourceFileName


            try
            {
                if (IsSecureEmail == "Y")
                    SendMailSubject = SendMailSubject + "-SECURE";
                MailMessage email = new MailMessage();
                SmtpClient SmtpServer = new SmtpClient(SmtpServerName);
                email.From = new MailAddress(SendMailFrom);
                email.To.Add(SendMailTo);


               // string[] fileEntries = Directory.GetFiles(SourceFolderPath, SourceFileName + "*");
                email.Subject = SendMailSubject;                

                DataTable dt = new DataTable();
                OleDbDataAdapter da = new OleDbDataAdapter();
                
                da.Fill(dt, Dts.Variables["User::rsFullFileNames"].Value);

                foreach (DataRow fileName in dt.Rows)
                {
                    string strfilename = fileName["FullFileNames"].ToString();
                    if (IsAttachFile == "Y")
                    {
                        email.Attachments.Add(new Attachment(strfilename));
                    }
                    else
                    {
                        SendMailBody = SendMailBody + "\n" + strfilename;

                    }
                    email.Body = SendMailBody;
                }
                if (dt.Rows.Count > 0)
                {
                    ServicePointManager.SecurityProtocol = (SecurityProtocolType)3072;
                    SmtpServer.Port = 587;
                    SmtpServer.Credentials = new System.Net.NetworkCredential(SendMailFrom, SendMailPswd);
                    SmtpServer.EnableSsl = true;
                    SmtpServer.Send(email);
                    email.Attachments.Dispose();
                }
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                MessageBox.Show(ex.Message.ToString());
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

