                //string[] fileEntries = Directory.GetFiles(SourceDirectory, lvFileName);
                string[] fileEntries = Directory.GetFiles(SourceDirectory, lvFileName);

                foreach (string filename in fileEntries)
                {
                    Console.WriteLine(filename);
                }


                if (FileDelimiter == "F")
                    {
                    string FileDelimiterx = "|";
                        foreach (string fileName in fileEntries)
                        {
                        var srx = new StreamReader(fileName);
                        string linex = srx.ReadLine();
                        string linefinal;
                        var dtx = new DataTable();
