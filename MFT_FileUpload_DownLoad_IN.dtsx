        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            string datetime = DateTime.Now.ToString("yyyyMMddHHmmss");
            try
            {
               
                SqlConnection myADONETConnection = new SqlConnection();
                myADONETConnection = (SqlConnection)
                //    (stringcon as SqlConnection)
               (Dts.Connections["NHSQLDBConnADO"].AcquireConnection(Dts.Transaction) as SqlConnection);

                //Declare Variables
                string lvintFileInfoId = Dts.Variables["User::intFileInfoId"].Value.ToString();
                string lvstrDirection = Dts.Variables["User::strDirection"].Value.ToString();
                string lvstrFileFormat = Dts.Variables["User::strFileFormat"].Value.ToString();
                string lvstrFileType = Dts.Variables["User::strFileType"].Value.ToString();
                string lvstrSnapshotFlag = Dts.Variables["User::strSnapshotFlag"].Value.ToString();
                string lvstrDataSource = Dts.Variables["User::strDataSource"].Value.ToString();
                string SourceFolderPath = Dts.Variables["User::strWinSCPDestinationPath"].Value.ToString();
                string FileExtension = Dts.Variables["User::strFileExtension"].Value.ToString();
                string lvFileName = Dts.Variables["User::strWinSCPSourceFileName"].Value.ToString();
                string FileName = Dts.Variables["User::strWinSCPSourceFileName"].Value.ToString() + "*." + Dts.Variables["User::strFileExtension"].Value.ToString();
                string passphrase = Dts.Variables["User::strPGPPassphrase"].Value.ToString();
                string PGPFileExtension = Dts.Variables["User::strPGPFileExtension"].Value.ToString();
                string PGPKeyName = Dts.Variables["User::strPGPKeyName"].Value.ToString();
                string ClientCode = Dts.Variables["User::strClientCode"].Value.ToString();
                string strIsPGP = Dts.Variables["User::strIsPGP"].Value.ToString();
                DateTime currentdatetime = Convert.ToDateTime(Dts.Variables["User::dtCurrentDatetime"].Value);
                string strFullFileNames = "";
                string FiletrackIDs = "";


                //Reading file names one by one
                string SourceDirectory = SourceFolderPath;
                string[] fileEntries = Directory.GetFiles(SourceDirectory, FileName, SearchOption.TopDirectoryOnly);
                object FiletrackID;
                //string[] fileEntries = Directory.GetFiles(SourceDirectory, FileName, SearchOption.TopDirectoryOnly).Where(x => new FileInfo(x).CreationTime.Date == DateTime.Today.Date).ToArray(); 
                // string[] fileEntries = Directory.GetFiles(SourceDirectory, FileName, SearchOption.TopDirectoryOnly).Where(x => new FileInfo(x).LastWriteTime >= currentdatetime).ToArray();
                foreach (string fileName in fileEntries)
                {

                    strFullFileNames = strFullFileNames + fileName.Replace(SourceFolderPath, "") + ",";
                    //Writing Data of File Into Table
                    //MessageBox.Show(fileName.Replace(SourceFolderPath, ""));

                    string delquery = "delete from mft.filetrack where FileName = '" + fileName.Replace(SourceFolderPath, "") + "' and StatusCode = '100'";
                    SqlCommand myCommand2 = new SqlCommand(delquery, myADONETConnection);
                    myCommand2.ExecuteNonQuery();
                    //"IF NOT EXISTS (SELECT 1 FROM mft.filetrack WHERE StatusCode=999 AND FileName ='" + fileName.Replace(SourceFolderPath, "") + "') "
                    string query = "IF NOT EXISTS (SELECT 1 FROM mft.filetrack WHERE 1=1 and StatusCode = '100' AND FileName ='" + fileName.Replace(SourceFolderPath, "") + "') "
                        + "BEGIN "
                        + "INSERT INTO mft.filetrack (FileInfoID,ClientCode,Direction, FileName, FileFormat,FileType,SnapshotFlag,DataSource,DateReceived,IsActive,StatusCode) "
                        + "VALUES (" + lvintFileInfoId + ",'" + ClientCode + "','" + lvstrDirection + "','" + fileName.Replace(SourceFolderPath, "")  + "','" + lvstrFileFormat + "','" + lvstrFileType + "','" + lvstrSnapshotFlag + "','" + lvstrDataSource + "', GETDATE(),1," + "'100' ) "
                        + "SELECT FiletrackID = @@Identity "
                        + "END "
                        + "ELSE "
                        + "BEGIN "
                        + "SELECT FiletrackID = 0 "
                        + "END ";

                    SqlCommand myCommand1 = new SqlCommand(query, myADONETConnection);
                    //myCommand1.ExecuteNonQuery();

                    FiletrackID = myCommand1.ExecuteScalar();
                    FiletrackIDs = FiletrackIDs + FiletrackID.ToString() + ",";

                    if (PGPFileExtension != "" && strIsPGP == "Y")
                    {
                        string command = "echo " + passphrase + "| gpg --pinentry-mode loopback --passphrase-fd 0  -o" + " --batch --yes --ignore-mdc-error  --output  " + fileName.Replace("." + PGPFileExtension, "") + " --decrypt --recipient " + PGPKeyName + " --decrypt " + fileName;
                        ExecuteCommand(command, true);
                        if (File.Exists(SourceFolderPath + "Archive\\" + fileName.Replace(SourceFolderPath, "")))
                        {
                            //File.Copy(SourceFolderPath + "Archive\\" + fileName.Replace(SourceFolderPath, ""), SourceFolderPath + "Archive\\" + fileName.Replace(SourceFolderPath, "")+"Copy"+DateTime.Now.ToString());
                            File.Move(fileName, SourceFolderPath + "Archive\\" + "Copy_" + DateTime.Now.ToString("ddMMyyyyhhmmss") + "_" + fileName.Replace(SourceFolderPath, ""));
                        }
                        else
                        {
                            File.Move(fileName, SourceFolderPath + "Archive\\" + fileName.Replace(SourceFolderPath, ""));
                        }
                    }
                    if (FiletrackID.ToString() == "0")
                    {
                        if (File.Exists(SourceFolderPath + "Archive\\" + fileName.Replace(SourceFolderPath, "").Replace("." + PGPFileExtension, "")))
                        {
                            File.Move(fileName, SourceFolderPath + "Archive\\" + "Copy_" + DateTime.Now.ToString("ddMMyyyyhhmmss")+"_"+ fileName.Replace(SourceFolderPath, "").Replace("." + PGPFileExtension, ""));// + "_Copy_" + DateTime.Now.ToString());
                        }
                      //File.Move(fileName.Replace("." + PGPFileExtension, ""), SourceFolderPath + "Archive\\" + fileName.Replace(SourceFolderPath, "").Replace("." + PGPFileExtension, ""));
                    }
                }

                if (strFullFileNames != "")
                    Dts.Variables["User::strFullFileNames"].Value = strFullFileNames.ToString().Remove(strFullFileNames.Length - 1, 1);
                if(FiletrackIDs != "")
                    Dts.Variables["User::FiletrackIDs"].Value = FiletrackIDs.ToString().Remove(FiletrackIDs.Length - 1, 1);

            }
            catch (Exception exception)
            {                
                MessageBox.Show(exception.Message.ToString());
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        public static string ExecuteCommand(string arguments, bool readOutput)
        {
            var output = string.Empty;
            try
            {
                var startInfo = new ProcessStartInfo
                {  
                    FileName = "cmd.exe",
                    Arguments = "/C " + arguments,
                    WindowStyle = ProcessWindowStyle.Hidden,
                    UseShellExecute = false,
                    CreateNoWindow = true,
                    RedirectStandardOutput = true,
                    RedirectStandardError = false
                };
                var proc = Process.Start(startInfo);
                if (readOutput)
                {
                    output = proc.StandardOutput.ReadToEnd();
                }

                return output;
            }
            catch (Exception)
            {
                return output;
            }
        }
    }

